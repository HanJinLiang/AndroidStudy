        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".systemwidget.SystemWidgetStudyActivity">
        </activity>
        <activity android:name=".systemwidget.coordinatorlayout.CoordinatorLayoutActivity">
        </activity>
        <activity android:name=".systemwidget.appbarlayout.ToolBarActivity"/>
        <activity android:name=".systemwidget.appbarlayout.AppBarLayoutActivity">
        </activity>
        <activity android:name=".customerviews.horizontalselectedview.HorizontalSelectedViewActivity">
        </activity>
        <activity android:name=".customerviews.CustomerViewStudyActivity"/>
        <activity android:name=".customerviews.CustomerProgressBar.CustomerProgressBarActivity">
        </activity>
        <activity android:name=".customerviews.curve.CurveActivity">
        </activity>
        <activity android:name=".customerviews.EraserView.EraserViewActivity">
        </activity>
        <activity android:name=".customerviews.EraserView.GuagualeViewActivity"/>
        <activity android:name=".customerviews.circleimage.CircleImageViewActivity">
        </activity>
    </application>

</manifest>